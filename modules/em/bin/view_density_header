#!/usr/bin/env python

import IMP.em


def main():
    IMP.set_log_level(IMP.SILENT)
    usage = "%prog [options] <em map> "
    p = IMP.ArgumentParser()
    p.add_argument("em_map", help="EM density map file name")

    args = p.parse_args()
    dmap = IMP.em.read_map(args.em_map)
    dmap.get_header().show()

if __name__ == "__main__":
    main()
